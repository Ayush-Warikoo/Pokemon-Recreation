void sceneAttack1()
{
  oppPokemon();
  Infernape();
   fill(255, 80, 80);
  rect(25, 400, 150, 100);
  fill(0);
  textSize(26);
  text("Flare", 50, 460);
  text("blitz", 50, 480);
  
  fill(69, 123, 255);
  rect(25, 500, 150, 100);
